    "pretestBAK": "npm run lint -s",
    "test": "mocha --harmony; NODE_ENV=test ava ./test-ava "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/energychain/StromDAO-BusinessObject.git"
  },
  "keywords": [
