  "license": "MIT",
  "bugs": {
    "url": "https://github.com/energychain/StromDAO-BusinessObject/issues"
  },
  "homepage": "https://github.com/energychain/StromDAO-BusinessObject#readme",
  "dependencies": {
    "ethers": "^2.0.3",
    "get-random-values": "^1.2.0",
    "node-persist": "^2.0.11"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "mocha": "^3.3.0",
    "promise-tool": "^1.0.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2"
